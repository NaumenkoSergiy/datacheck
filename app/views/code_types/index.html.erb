<h1>Listing code_types</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @code_types.each do |code_type| %>
      <tr>
        <td><%= link_to 'Show', code_type %></td>
        <td><%= link_to 'Edit', edit_code_type_path(code_type) %></td>
        <td><%= link_to 'Destroy', code_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Code type', new_code_type_path %>
